<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      style="-fx-background-color: #202130"
      fx:controller="com.mas.joan.focussession.Controller.OverviewController"
      prefHeight="488" prefWidth="369">
    <padding>
        <Insets topRightBottomLeft="10" />
    </padding>
    <Label text="Closed Tasks" textFill="WHITE">
        <font>
            <Font size="20.0" />
        </font></Label>
    <HBox spacing="10">
        <Label text="Search:"/>
        <TextField fx:id="searchInput" promptText="Enter product"/>
    </HBox>
    <TableView fx:id="closedTaskView">
        <columns>
            <TableColumn fx:id="closedTitleRow" prefWidth="70.99999558925629" text="Task">
            </TableColumn>
            <TableColumn fx:id="closedDescriptionRow" prefWidth="159.40003204345703" text="Description">
            </TableColumn>
            <TableColumn fx:id="closedTimeRow" prefWidth="118.80000305175781" text="Time Elapsed">
            </TableColumn>
        </columns>
    </TableView>
</VBox>
